{
    "name": "dev-jm/nova-dependent-filter",
    "description": "Cascading/dependent select filters for Laravel Nova 5.",
    "keywords": [
        "laravel",
        "nova",
        "filter",
        "dependent",
        "cascading"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "DevJM\\DependentFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevJM\\DependentFilter\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
